prefix ids: <https://w3id.org/ids/core/>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix ids_vis: <https://schema.iais.fraunhofer.de/ids/helper/visualization/>

template ids:process-focusclass-to-uml {

    "\n"

    "class " str(?label) " \n"

}
where {
    ?class ids_vis:focusClass true;
        rdfs:label ?label.
}


