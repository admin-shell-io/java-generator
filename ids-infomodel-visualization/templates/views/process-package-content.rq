prefix ids: <https://w3id.org/ids/core/>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix dct: <http://purl.org/dc/terms/>
prefix ids_vis: <https://schema.iais.fraunhofer.de/ids/helper/visualization/>

template ids:process-package-content(?package) {

    "\n"
    "\tclass " str(?label)
}
where {
    ?class ids_vis:belongsToPackage ?package;
        a owl:Class;
        rdfs:label ?label.
}



