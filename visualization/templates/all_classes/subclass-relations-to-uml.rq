prefix ids: <https://w3id.org/idsa/core/>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

template ids:subclass-relations-to-uml {

    "\n"
    str(?classAlabel) " --|> " str(?classBlabel) "\n"

}
where {
    ?classA rdfs:subClassOf ?classB.

    ?classA rdfs:label ?classAlabel.
    ?classB rdfs:label ?classBlabel.

    FILTER (strstarts(str(?classA), str(<ids:>)) && strstarts(str(?classB), str(<ids:>)))
}

