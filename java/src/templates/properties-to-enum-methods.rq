prefix ids: <https://w3id.org/idsa/core/>
prefix idsm: <https://w3id.org/idsa/metamodel/>
prefix idstt: <https://w3id.org/idsa/transformationtemplates/>

template ids:properties-to-enum-methods(?class) {

	# Iterates on selected properties and writes getter and setter methods

    "\n"
    "\n\t@JsonIgnore"
	"\n\t"

    # visibility modifier
    "final public "

    # signature
	st:call-template(ids:property-to-getter-method, ?property, ?class)

	" {\n\t"
	"\t//not implemented for enums\n\t"
	"\tthrow new UnsupportedOperationException();\n\t"
	"}\n\t"

}
where  {   
    ?property rdfs:domain ?class.
}
