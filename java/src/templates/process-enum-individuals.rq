prefix ids: <https://w3id.org/ids/core/>
prefix dct: <http://purl.org/dc/terms/>
prefix idsm: <https://w3id.org/ids/metamodel/>

template ids:process-enum-individuals(?class) {

    if (bound(?comment),
        concat("\n\t/** ", str(?comment), " */"),
        '')

    "\n\t" str(?identifier) "(\"" str(?individual) "\"),"
}
where  {
    ?individual a ?class;
        idsm:constantIdentifier ?identifier.

    OPTIONAL {
        ?individual rdfs:comment ?comment.
    }
}
