prefix idsv: <http://industrialdataspace.org/2016/10/idsv/core#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

template idsv:process-class(?class) {

    st:call-template(idsv:process-class-to-enum, ?class, ?comment)
    st:call-template(idsv:process-class-to-interface, ?class, ?comment)

}
where{
    ?class a owl:Class;
        rdfs:comment ?comment.
}
