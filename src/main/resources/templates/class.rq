prefix idsv: <http://industrialdataspace.org/2016/10/idsv/core#>
prefix dcat: <http://www.w3.org/ns/dcat#>

template{

	# Write process result into an own file
	kg:write(concat("/tmp/",strafter(str(?class), "#"), ".java"),
		st:call-template(idsv:process-class, ?class))

	kg:write(concat("/tmp/",strafter(str(?class), "#"), "Impl.java"),
		st:call-template(idsv:process-class-to-class, ?class))
}
where  {
    ?class a owl:Class.
}
