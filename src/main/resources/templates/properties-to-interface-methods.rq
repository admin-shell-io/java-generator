prefix ids: <http://industrialdataspace.org/2016/10/ids/core#>

template ids:properties-to-interface-methods(?class) {

	# Iterates on selected properties and writes getter and setter methods

    # getter method signature
    "\n\t"
    "@de.fhg.iais.jrdfb.annotation.RdfProperty(\"" str(?property) "\")"
	"\n\t" st:call-template(ids:property-to-getter-method, ?property, ?class) ";"

}
where  {   
  ?property rdfs:domain ?class;
    rdfs:label ?label.
}
