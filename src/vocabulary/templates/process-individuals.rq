prefix ids: <http://industrialdataspace.org/2016/10/ids/core#>
prefix dct: <http://purl.org/dc/terms/>

template ids:process-individuals(?class) {

    if (bound(?comment),
        concat("\n\t/* ", str(?comment), " */"),
        '')

    "\n\t@de.fhg.iais.jrdfb.annotation.RdfId(uriTemplate = \"" str(?individual) "\")"
    "\n\t" str(?identifier) ","

}
where  {
    ?individual a ?class;
        dct:identifier ?identifier.

    OPTIONAL {
        ?individual rdfs:comment ?comment.
    }
}
